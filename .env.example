# Copy to .env (same folder) and set your Tebex store URL
VITE_TEBEX_STORE_URL=https://outcraft.tebex.io

# Optional: map local product IDs to Tebex package IDs for direct redirect
# Format: VITE_TEBEX_PKG_<PRODUCT_ID_IN_UPPERCASE_WITH_UNDERSCORES>=<number>
# Examples:
# VITE_TEBEX_PKG_VIP_SUB=123456
# VITE_TEBEX_PKG_VIP_MONTH=234567
# VITE_TEBEX_PKG_VIP_LIFETIME=345678
# VITE_TEBEX_PKG_WARRIOR=456789
# VITE_TEBEX_PKG_KNIGHT=567890
# VITE_TEBEX_PKG_LEGEND=678901
# VITE_TEBEX_PKG_IMMORTAL=789012
# VITE_TEBEX_PKG_COINS_1K=890123
# VITE_TEBEX_PKG_COINS_2K=901234
# VITE_TEBEX_PKG_COINS_5K=912345
# VITE_TEBEX_PKG_COINS_8K=923456
# VITE_TEBEX_PKG_COINS_14K=934567
